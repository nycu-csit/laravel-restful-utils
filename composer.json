{
    "name": "nycu-csit/laravel-restful-utils",
    "description": "Restful API utilities for Laravel projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NycuCsit\\LaravelRestfulUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Shih-Tse Chang",
            "email": "stchang@it.cs.nycu.edu.tw"
        }
    ],
    "require": {
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "php": ">=8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NycuCsit\\LaravelRestfulUtils\\RestfulUtilServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8"
    }
}
